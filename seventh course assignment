#include<bits/stdc++.h>

using namespace std;
const int maxn = 1e5 + 10;
typedef long long ll;

int n, m;
int c[maxn], w[maxn], dp[maxn];
void backpack_01(){
	memset(dp, 0, sizeof dp);
	for(int i = 0; i < n; ++ i) {
		printf("第%d个项目: cost = %d 收益 = %d\n", i + 1, c[i], w[i]);
		for(int j = m; j >= c[i]; -- j) {
			printf("dp[%d] = %d -> ", j, dp[j]);
			dp[j] = max(dp[j], dp[j - c[i]] + w[i]);
			printf("dp[%d] = %d\n", j, dp[j]);
		}
		puts("");
	}
}

signed main(){
	scanf("%d %d", &n, &m);
	//n为项目个数，m为资金	
	for(int i = 0; i < n; ++ i)	
		scanf("%d %d", &c[i], &w[i]);
	//c[i]为花费，w[i]为收益 
	backpack_01();
	printf("最大收益为：%d\n", dp[m]); 
	return 0;
} 
